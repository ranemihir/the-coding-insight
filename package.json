{
  "name": "the-coding-insight",
  "version": "1.0.0",
  "description": "The Coding Insight is a blogging website about technology and computer programming.",
  "author": "ranemihir",
  "keywords": [
    "the coding insight",
    "programming",
    "blog",
    "vuejs",
    "nuxtjs",
    "editorjs",
    "material design",
    "server side rendering"
  ],
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/server/index.js --watch server",
    "start": "cross-env NODE_ENV=production node src/server/index.js",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "build:firebase": "yarn clean && yarn build && yarn copy && cd functions",
    "clean": "yarn clean:public && yarn clean:functions && yarn clean:static",
    "clean:functions": "rimraf \"functions/.nuxt\"",
    "clean:public": "rimraf \"public/**/*.*!(md)\" && rimraf \"public/_nuxt\"",
    "clean:static": "rimraf \"src/static/sw.js\"",
    "copy": "yarn copy:nuxt && yarn copy:static",
    "copy:nuxt": "mkdir public/_nuxt && cp -r functions/.nuxt/dist/* public/_nuxt",
    "copy:static": "cp -r src/static/* public",
    "start:firebase": "firebase serve --only functions,hosting",
    "deploy": "firebase deploy --only functions,hosting",
    "generate:routes": "nuxt generate"
  },
  "dependencies": {
    "@editorjs/code": "^2.4.1",
    "@editorjs/editorjs": "^2.17.0",
    "@editorjs/embed": "^2.3.1",
    "@editorjs/header": "^2.4.1",
    "@editorjs/image": "^2.3.4",
    "@editorjs/inline-code": "^1.3.1",
    "@editorjs/list": "^1.4.0",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/paragraph": "^2.6.1",
    "@groupher/editor-code": "^1.0.25",
    "@mdi/js": "^5.6.55",
    "@rmdwirizki/editor-alert": "^1.0.20",
    "autosize": "^4.0.2",
    "axios": "^0.19.2",
    "cookie-parser": "^1.4.5",
    "cookie-universal-nuxt": "^2.1.4",
    "copy-to-clipboard": "^3.3.1",
    "cross-env": "^5.2.0",
    "editorjs-github-gist-plugin": "^1.0.4",
    "editorjs-math": "^1.0.14",
    "express": "^4.16.4",
    "firebase": "^7.15.5",
    "jsdom": "^16.2.2",
    "nuxt": "^2.14.5",
    "undom": "^0.4.0",
    "uuid": "^8.1.0",
    "vue": "^2.6.11",
    "vue-client-only": "^2.0.0",
    "vue-embed-gist": "^1.0.2",
    "vue-meta": "^2.3.4"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "file-loader": "^6.1.0",
    "image-webpack-loader": "^7.0.0",
    "nodemon": "^1.18.9"
  }
}
